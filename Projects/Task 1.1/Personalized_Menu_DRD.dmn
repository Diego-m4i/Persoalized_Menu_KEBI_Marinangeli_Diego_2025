    <dmn:informationRequirement triso:readOnly="false">
      <dmn:requiredInput href="#_f931f295-5556-46ee-a4b5-2c952e4e22b9"/>
    </dmn:informationRequirement>
    <dmn:informationRequirement triso:readOnly="false">
      <dmn:requiredDecision href="#_84b93d17-7c7c-4479-b8cd-76e2c4c420e8"/>
    </dmn:informationRequirement>
    <dmn:decisionTable hitPolicy="UNIQUE" id="_561d56dd-97fb-4a54-ad6f-c70737371d9f" outputLabel="Decide dish by filtering by Allergies" triso:descriptionVisible="false" triso:expressionId="_2718918e-dc83-4b65-ba76-77dbe8906934" typeRef="DishList">
      <dmn:input id="_95b00f84-487e-468c-81d9-bce1da4a1fb9" label="Host allergies" triso:allowNull="false">
        <dmn:inputExpression typeRef="AllergiesList">
          <dmn:text>Host allergies</dmn:text>
        </dmn:inputExpression>
        <dmn:inputValues triso:constraintsType="enumeration">
          <dmn:text>"lactose","gluten","eggs","nuts","soy","fish"</dmn:text>
        </dmn:inputValues>
      </dmn:input>
      <dmn:output id="_333cbe33-e7c1-4e08-94ed-a9a723dbd2b0" typeRef="DishList"/>
      <dmn:rule id="_5b9daa61-c5a2-4af1-8909-99e3a052d50b">
        <dmn:inputEntry id="_ff8b14d9-1d95-42a6-9cd4-1e6af1272ff9">
          <dmn:text>(for a in (if "Host allergies" instance of list then "Host allergies"
else ["Host allergies"]) return a )</dmn:text>
        </dmn:inputEntry>
        <dmn:outputEntry id="_4d66a87c-2dce-4a41-af40-f81d5a65fa59">
          <dmn:text>"Menu"[ not(some i in "item.allergens" satisfies i in
"Host allergies") ]</dmn:text>
        </dmn:outputEntry>
      </dmn:rule>
    </dmn:decisionTable>
  </dmn:decision>
